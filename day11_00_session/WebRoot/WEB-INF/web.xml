<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>	
  
  <servlet>
  		<servlet-name>codeServlet</servlet-name>
  		<servlet-class>com.heima.servlet.ServletResponse3</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.heima.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>MainServlet</servlet-name>
    <servlet-class>com.heima.servlet.MainServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RegisterServletUI</servlet-name>
    <servlet-class>com.heima.token.RegisterServletUI</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ResiterServlet</servlet-name>
    <servlet-class>com.heima.token.ResiterServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ServletSession1</servlet-name>
    <servlet-class>com.heima.url.ServletSession1</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ServletSession2</servlet-name>
    <servlet-class>com.heima.url.ServletSession2</servlet-class>
  </servlet>






  
  <servlet-mapping>
  		<servlet-name>codeServlet</servlet-name>
  		<url-pattern>/servlet/ServletResponse3</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/servlet/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>MainServlet</servlet-name>
    <url-pattern>/servlet/MainServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RegisterServletUI</servlet-name>
    <url-pattern>/servlet/RegisterServletUI</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ResiterServlet</servlet-name>
    <url-pattern>/servlet/ResiterServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ServletSession1</servlet-name>
    <url-pattern>/servlet/ServletSession1</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ServletSession2</servlet-name>
    <url-pattern>/servlet/ServletSession2</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>

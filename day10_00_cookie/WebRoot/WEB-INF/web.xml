  </servlet>
  <servlet>
    <servlet-name>ServletUI</servlet-name>
    <servlet-class>com.heima.one.ServletUI</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.heima.one.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>MainServlet</servlet-name>
    <servlet-class>com.heima.one.MainServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ShowAllBookServlet</servlet-name>
    <servlet-class>com.heima.two.servlet.ShowAllBookServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ShowBookDetailServlet</servlet-name>
    <servlet-class>com.heima.two.servlet.ShowBookDetailServlet</servlet-class>
  </servlet>








  <servlet-mapping>
    <servlet-name>ServletCookie1</servlet-name>
    <url-pattern>/servlet/ServletCookie1</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ServetCookie2</servlet-name>
    <url-pattern>/servlet/ServletCookie2</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ServletCookie3</servlet-name>
    <url-pattern>/ServletCookie3</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ServletUI</servlet-name>
    <url-pattern>/servlet/ServletUI</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/servlet/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>MainServlet</servlet-name>
    <url-pattern>/servlet/MainServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ShowAllBookServlet</servlet-name>
    <url-pattern>/servlet/ShowAllBookServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ShowBookDetailServlet</servlet-name>
    <url-pattern>/servlet/ShowBookDetailServlet</url-pattern>
  </servlet-mapping>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>

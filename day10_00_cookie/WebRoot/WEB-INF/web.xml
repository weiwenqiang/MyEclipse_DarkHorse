<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>
  <servlet>
    <servlet-name>ServletCookie1</servlet-name>
    <servlet-class>com.heima.cookie.ServletCookie1</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ServetCookie2</servlet-name>
    <servlet-class>com.heima.cookie.ServletCookie2</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ServletCookie3</servlet-name>

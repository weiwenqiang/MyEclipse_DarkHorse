<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>
  <servlet>
    <servlet-name>ServletSession1</servlet-name>
    <servlet-class>com.heima.session.ServletSession1</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ServletSession2</servlet-name>
    <servlet-class>com.heima.session.ServletSession2</servlet-class>
  </servlet>
  
  <servlet>
  	<servlet-name>ShowAllBookServlet</servlet-name>
  	<servlet-class>com.heima.car.servlet.ShowAllBookServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ShowBookDetailServlet</servlet-name>
  	<servlet-class>com.heima.car.servlet.ShowBookDetailServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>BuyServlet</servlet-name>
    <servlet-class>com.heima.car.servlet.BuyServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CarServlet</servlet-name>
    <servlet-class>com.heima.car.servlet.CarServlet</servlet-class>
  </servlet>




	<servlet-mapping>
		<servlet-name>ShowBookDetailServlet</servlet-name>
		<url-pattern>/servlet/ShowBookDetailServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ShowAllBookServlet</servlet-name>
		<url-pattern>/servlet/ShowAllBookServlet</url-pattern>
	</servlet-mapping>
  <servlet-mapping>
    <servlet-name>ServletSession1</servlet-name>
    <url-pattern>/servlet/ServletSession1</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ServletSession2</servlet-name>
    <url-pattern>/servlet/ServletSession2</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>BuyServlet</servlet-name>
    <url-pattern>/servlet/BuyServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CarServlet</servlet-name>
    <url-pattern>/servlet/CarServlet</url-pattern>
  </servlet-mapping>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
